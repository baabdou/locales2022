{% extends 'base.html.twig' %}

{% block title %}Liste{% endblock %}

{% block body %}
    <h1>Liste</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ liste.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ liste.libelle }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ liste.description }}</td>
            </tr>
            <tr>
                <th>Titulaires</th>
                <td>{{ liste.titulaires }}</td>
